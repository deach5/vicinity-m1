function buttonEvent(e){
	type = e.type
	aButton = this

	
	if(aButton.mouseEnable){
		if(e.type == 'mouseover' ||e.type == 'focusin' ||e.type == 'mouseenter' ){
			
			if(aButton.myFeedbackClass != '#section_5 .caption'){
				$(aButton).css({backgroundPosition:aButton.state_1})	
			}
			else{
				$(aButton).css({opacity:.4})	
			}
			
		}
		if(e.type == 'mouseout' || e.type == 'focusout' ||e.type == 'mouseleave' ){		
			
			
			if(aButton.myFeedbackClass != '#section_5 .caption'){
				$(aButton).css({backgroundPosition:aButton.state_0})	
			}
			else{
				$(aButton).css({opacity:0})	
			}
			
							
		}
	}
	
	
	if(e.type == 'click' && aButton.mouseEnable){
		
	if($(aButton).data('mp3')){	
		$('.playBtn').removeClass('on')
		swapAudio($(aButton).data('mp3'))
	}
		
	if(aButton.myFeedbackClass == '#section_1 .caption'){
			$(aButton.myFeedbackClass).removeClass('slideFromLeft')
			$(aButton.myFeedback).addClass('slideFromLeft')
			$(aButton.myTick).show()
			if(aButton.checkClick()){
			
				$('#section_1 .checkBox').addClass('isactive').each(function(index, element) {
                    this.disableThis(false)
                });
			}
	}
		
	//section 1 buttons	events
	if(aButton.myFeedbackClass == '#section_1 .checkbox_fb'){
			
			aButton.$myGroup.css({backgroundPosition:aButton.state_0})
			$(aButton).css({backgroundPosition:aButton.state_2})
			aButton.disableAll(true)	
			$('#section_1 .feedback').fadeIn()					
			completeSections($('#section_1'))
			
	}
		
		
		//section 5 buttons events
	if(aButton.myFeedbackClass == '#section_5 .caption'){
			
			$(aButton.myFeedbackClass).removeClass('slideFromLeft')
			$(aButton.myFeedback).addClass('slideFromLeft')
			
	
			if(aButton.checkClick()){
	
				$('#s5_box2 .pointers').addClass('fadeIn')
				$('#s5_box2 .pointers2').addClass('fadeIn')
				completeSections($('#section_5'))
			}
			
		}
	}
}


</script>
